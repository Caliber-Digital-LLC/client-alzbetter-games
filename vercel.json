{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/public",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/checkers/:path*",
      "destination": "/checkers/:path*"
    },
    {
      "source": "/slots/:path*",
      "destination": "/slots/:path*"
    },
    {
      "source": "/blackjack/:path*",
      "destination": "/blackjack/:path*"
    },
    {
      "source": "/roulette/:path*",
      "destination": "/roulette/:path*"
    },
    {
      "source": "/poker/:path*",
      "destination": "/poker/:path*"
    },
    {
      "source": "/holdem/:path*",
      "destination": "/holdem/:path*"
    },
    {
      "source": "/craps/:path*",
      "destination": "/craps/:path*"
    },
    {
      "source": "/wordchange/:path*",
      "destination": "/wordchange/:path*"
    },
    {
      "source": "/solitaire/:path*",
      "destination": "/solitaire/:path*"
    },
    {
      "source": "/shared/:path*",
      "destination": "/shared/:path*"
    },
    {
      "source": "/((?!checkers|slots|blackjack|roulette|poker|holdem|craps|wordchange|solitaire|shared).*)",
      "destination": "/index.html"
    }
  ]
}
